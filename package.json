{
  "name": "monomongo-lemur",
  "version": "0.3.0",
  "main": "dist/server/index.js",
  "browser": "dist/browser/index.js",
  "description": "MonoMongo es una biblioteca para gestionar conexiones con bases de datos MongoDB utilizando Mongoose, facilitando el manejo de entornos locales y de producci√≥n.",
  "scripts": {
    "browser-watch": "npx tsc --outDir dist/browser --module ES6 -w",
    "server-watch": "npx tsc --outDir dist/server -w",
    "build-cdn": "npx tsc --outDir dist/cdn --module ESNext --target ES5",
    "build-browser": "npx tsc --outDir dist/browser --module ES6",
    "build-server": "npx tsc --outDir dist/server",
    "build": "npm run build-browser & npm run build-server"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "monomongo-lemur",
    "lemur"
  ],
  "author": {
    "name": "elkin chaverra",
    "email": "elkincp5@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ElkinCp5/monomongo-lemur"
  },
  "license": "MIT",
  "dependencies": {
    "mongoose": "8.9.5"
  },
  "devDependencies": {
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.5.3"
  },
  "homepage": "https://github.com/ElkinCp5/monomongo-lemur#readme"
}
